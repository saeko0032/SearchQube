<Window x:Class="SearchQube.Views.BurrowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Burrow Equipment" Height="350" Width="525">
    <Grid>
        <TextBlock Text="Burrow Equipment + ScanRFID" FontSize="24" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,30,0,0"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="OK" Width="100" Height="30" Margin="0,10,0,0" Click="OkButton_Click"/>
        </StackPanel>
        <Button Content="Back" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="100" Height="30" Margin="0,0,0,30" Click="BackButton_Click"/>
    </Grid>
</Window>
